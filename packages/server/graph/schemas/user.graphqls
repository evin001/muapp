enum Role {
  master
  client
}

type User {
  id: String!
  firstName: String
  lastName: String
  email: String!
  phone: String!  
  emailVerified: Boolean!
  phoneVerified: Boolean!
  role: Role!
  authToken: String!
  refreshToken: String!
}

type Tokens {
  authToken: String!
  refreshToken: String!
}