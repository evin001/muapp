type ScheduleEvent {
  id: Int!
  date: Date!
  intervalStart: String!
  intervalEnd: String!
  type: ScheduleEventType!
  services: [Int]
  color: String
  userId: Int!
  code: String!
}

input ScheduleEventInput {
  id: Int
  date: Date!
  intervalStart: String!
  intervalEnd: String!
  type: ScheduleEventType!
  services: [Int]
  color: String
}

enum ScheduleEventType {
  once
  daily
  weekly
  monthly
  weekday
}